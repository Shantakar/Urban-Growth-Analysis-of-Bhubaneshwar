/***** End of imports. If edited, may not auto-convert in the playground. *****/
// Define scaling factor and offset for image preprocessing
var scalingFactor = 0.0000275;
var offset = -0.2;

// Center the map on the Area of Interest (AOI)
Map.centerObject(AOI, 10);

// Load Landsat imagery and apply preprocessing
var LandsatImageOriginal = ee.ImageCollection('LANDSAT/LC08/C02/T1_L2')
    .filterBounds(AOI)
    .filterDate('2013-01-01', '2013-12-31')
    .select(['SR_B1', 'SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B7'])
    .filterMetadata('CLOUD_COVER', 'less_than', 2)
    .reduce(ee.Reducer.median());

// Print original Landsat image
print('LandsatImageOriginal', LandsatImageOriginal);

// Rename image bands for clarity
var Renamed_Image = LandsatImageOriginal.select(
    ['SR_B1_median', 'SR_B2_median', 'SR_B3_median', 'SR_B4_median', 'SR_B5_median', 'SR_B7_median'],
    ['SR_B1', 'SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B7']
);

// Clip the image to the Area of Interest and convert to integer
var clip_image = Renamed_Image.clip(AOI).int();

// Apply scaling and offset to the clipped image
clip_image = clip_image.multiply(scalingFactor).add(offset);

// Add the clipped image to the map with specified visualization parameters
Map.addLayer(clip_image, {bands: ['SR_B5', 'SR_B4', 'SR_B3'], min: 0, max: 3000}, 'clip_image (RGB)');
Map.addLayer(clip_image, {bands: ['SR_B4', 'SR_B3', 'SR_B2'], min: 0, max: 3000}, 'clip_image (TCC)');

// Define training points and sample training data from the clipped image
var training_points = Builtup.merge(Vegetation).merge(Waterbody).merge(Cropland).merge(Riverbed).merge(Barrenland);
var training_data = clip_image.sampleRegions({
    collection: training_points,
    properties: ['Class'],
    scale: 30
});

// Add a random column for splitting into training and validation samples
training_data = training_data.randomColumn();
var training_split = 0.8;
var training_samples = training_data.filter(ee.Filter.lt('random', training_split));
var validation_samples = training_data.filter(ee.Filter.gte('random', training_split));

// Train a Random Forest classifier using training samples
var classifier = ee.Classifier.smileRandomForest(50).train({
    features: training_samples,
    classProperty: 'Class',
    inputProperties: ['SR_B1', 'SR_B2', 'SR_B3', 'SR_B4', 'SR_B5', 'SR_B7']
});
// var classifier = ee.Classifier.smileCart(290).train({
//     features: training_samples,
//     classProperty: 'Class',
//     inputProperties: ['SR_B1','SR_B2','SR_B3','SR_B4','SR_B5','SR_B7']
// });
var validation_classified = validation_samples.classify(classifier);

//Calculate the error matrix and accuracies
var trainConfusionMatrix = classifier.confusionMatrix();
var trainKappa = trainConfusionMatrix.kappa();
print('Training Kappa', trainKappa);
var errorMatrix = validation_classified.errorMatrix('Class', 'classification');
print('Training Overall Accuracy: ', trainConfusionMatrix.accuracy());
var validationErrorMatrix = validation_classified.errorMatrix('Class', 'classification');
var overallAccuracy = errorMatrix.accuracy();
var producersAccuracy = errorMatrix.producersAccuracy();
var usersAccuracy = errorMatrix.consumersAccuracy();
var kappa = errorMatrix.kappa();
print('Validation Error Matrix RF: ', validationErrorMatrix);
print('Validation Overall Accuracy RF: ', validationErrorMatrix.accuracy());

//Add the classified image to the map
var classified_image_2013 = clip_image.classify(classifier);
Map.addLayer(classified_image_2013, {min: 0, max: 5, palette: ['Red','Green','Blue','9CC860','Cyan','Pink']}, 'classified_image');

// Print the error matrix, accuracies, and Kappa statistic
print('Error Matrix', errorMatrix);
print('Overall Accuracy', overallAccuracy);
print('Producer\'s Accuracy', producersAccuracy);
print('User\'s Accuracy', usersAccuracy);
print('Kappa Statistic', kappa);



// Area Under Each Class //

var areaImage = ee.Image.pixelArea().divide(1000000).addBands(classified_image_2013);
var areas = areaImage.reduceRegion({
  reducer: ee.Reducer.sum().group({
    groupField: 1,
    groupName: 'Class',
  }),
  geometry: AOI,
  scale: 30,
  maxPixels: 1e9
});
print('Area by class (sq km)', areas);

var areaOfAOI = AOI.geometry().area().divide(1000000); // in square kilometers
print('Area of AOI:', areaOfAOI);

Export.image.toDrive({
  image: classified_image_2013,
  description: 'classified_image_2013',
  region:AOI,
  scale: 30,
  folder:'ProjectBigData',
  maxPixels: 1e9,
});

// Create a legend panel
var classColors = ['#FF0000', '#00FF00', '#0000FF', '#9CC860', '#00FFFF', '#FFC0CB']; // Hexadecimal color codes for classes (Red, Green, Blue, Light Green, Cyan, Pink)
var classNames = ['Built-up', 'Forest/Vegetation', 'Waterbody', 'Cropland', 'Riverbed', 'Barrenland']; // Class names based on your training data


var legend_panel = ui.Panel({
  style: {
    position: 'bottom-left',
    padding: '6px'
  }
});

// Add legend title to the panel
var legend_title = ui.Label({
  value: "Land Use / Land Cover",
  style: {
    fontSize: '14px',
    fontWeight: 'bold',
    margin: '0px'
  }
});
legend_panel.add(legend_title);

// Function to create legend items
var list_legend = function(color, description) {
  var color_box = ui.Label({
    style: {
      backgroundColor: color, // Use the provided color directly
      padding: '10px',
      margin: '4px'
    }
  });
  var class_label = ui.Label({
    value: description,
    style: {
      margin: '4px'
    }
  });
  return ui.Panel({
    widgets: [color_box, class_label],
    layout: ui.Panel.Layout.Flow('horizontal')
  });
};

// Add legend items for each class to the legend panel
for (var i = 0; i < classColors.length; i++) {
  legend_panel.add(list_legend(classColors[i], classNames[i]));
}

// Add legend panel to the Map
Map.add(legend_panel);